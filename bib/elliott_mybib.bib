@article{kotek_covert_2016,
	title = {Covert Pied-Piping in English Multiple Wh-Questions},
	volume = {47},
	issn = {0024-3892},
	url = {http://www.mitpressjournals.org/doi/full/10.1162/LING_a_00226},
	doi = {10.1162/LING_a_00226},
	abstract = {In this article, we argue for the existence of covert pied-piping in wh-questions through a previously unnoticed pattern of intervention effects in Superiority-obeying English multiple wh-questions. We show that the preference of covert pied-piping, unlike that of overt pied-piping, is for movement of larger constituents. We argue that this discrepancy stems from conflicting requirements of {PF} and {LF}: overt pied-piping feeds both {LF} and {PF}, but covert pied-piping feeds {LF} only. The study of covert pied-piping thus reveals the true preference of {LF} and narrow syntax with regard to pied-piping: larger pied-piping constituents are preferred over smaller ones. This preference can be overridden by certain {PF} constraints that apply to overt pied-piping.},
	pages = {669--693},
	number = {4},
	journaltitle = {Linguistic Inquiry},
	shortjournal = {Linguistic Inquiry},
	author = {Kotek, Hadas and Erlewine, Michael Yoshitaka},
	urldate = {2016-12-02},
	date = {2016-10-01}}

@article{cheng_trapped_2010,
	title = {Trapped at the edge: On long-distance pair-list readings},
	volume = {120},
	issn = {0024-3841},
	url = {http://www.sciencedirect.com/science/article/pii/S0024384109000618},
	doi = {10.1016/j.lingua.2008.07.008},
	series = {Exploring the Left Periphery},
	shorttitle = {Trapped at the edge},
	abstract = {This paper discusses a pair-list reading which we call the trapped pair-list reading, arising in long distance questions with three wh-phrases, two of which are embedded within a subordinate clause. Under the trapped pair-list reading, all three whs are answered, but only the two whs trapped inside the embedded clause can be paired. Based on evidence from a language with (overt) multiple wh-movement (Romanian), we argue that (lists of) trapped pair-list readings in English involve covert multiple partial wh-movement. On this proposal, trapped pair-list readings raise, for multiple partial wh-movement, the same canonical scope paradox that single partial wh-movement raises, since on the relevant list reading, all three whs have semantically wide, matrix scope, though syntactically, the (only) two whs that can be paired are trapped at the periphery of the subordinate clause. We suggest that this paradox can be resolved by extending Sternefeld (2001) and Lipták and Zimmermann's (2007) Indirect Dependency Approach to single partial-movement to multiple partial-movement. The basic insight driving this proposal is that the subordinate whs can be paired independently of the matrix wh because the subordinate clause can be interpreted as a multiple question on its own.},
	pages = {463--484},
	number = {3},
	journaltitle = {Lingua},
	shortjournal = {Lingua},
	author = {Cheng, Lisa Lai-Shen and Demirdache, Hamida},
	urldate = {2016-12-02},
	date = {2010-03-01},
	keywords = {Choice functions, Covert movement, Indirect dependency, Island effects, Long-distance questions, Multiple movement, Multiple questions, Pair list/triple list readings, Partial-movement, Successive cyclicity}}

