@thesis{urk_uniform_2015,
	title = {A uniform syntax for phrasal movement : a case study of Dinka Bor},
	rights = {http://dspace.mit.edu/handle/1721.1/7582},
	url = {http://dspace.mit.edu/handle/1721.1/101595},
	shorttitle = {A uniform syntax for phrasal movement},
	abstract = {This dissertation argues that all instances of phrasal movement are the result of Agree and Merge (Chomsky 1995, 2001), and, in addition to this, that the existence of different types of movement derives solely from variation in properties of the feature involved in the Agree relation. Guided by novel data from the Nilotic language Dinka Bor (South Sudan), I apply this view to three kinds of movement: A-movement, A-movement, and intermediate movement steps of successive-cyclic dependencies. In addition, I present a novel argument, from patterns of pronoun copying, that all phrasal movement arises from Merge.},
